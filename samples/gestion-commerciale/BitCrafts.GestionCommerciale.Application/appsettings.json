{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] (ThreadId: {ThreadId}) (ProcessId: {ProcessId}) {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] (ThreadId: {ThreadId}) (ProcessId: {ProcessId}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithProcessId"
    ]
  },
  "ApplicationSettings": {
    "Id": "com.bit-crafts.gestion-commerciale",
    "Name": "Gestion Commerciale",
    "Version": "1.0.0",
    "Author": "BitCrafts",
    "Type": "console"
  },
  "DatabaseSettings": {
    "Provider": "sqlite",
    "Server": "",
    "Database": "gestion-commerciale.sqlite",
    "User": "",
    "Password": ""
  },
  "ModulesPath": "./Modules"
}